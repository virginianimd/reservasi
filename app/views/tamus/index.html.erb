<%= render partial: 'structures/navigation' %>  

<p id="notice"><%= notice %></p>

<h1>Tamus</h1>

<table>
  <thead>
    <tr>
      <th>Kodebooking</th>
      <th>Namatamu</th>
      <th>Qty</th>
      <th>Tanggal</th>
      <th>Hotel</th>
      <th>Negara</th>
      <th>Telp</th>
      <th>Email</th>
      <th>Note</th>
      <th>Jenispaket</th>
      <th>Namaaktifitas</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tamus.each do |tamu| %>
      <tr>
        <td><%= tamu.kodebooking %></td>
        <td><%= tamu.namatamu %></td>
        <td><%= tamu.qty %></td>
        <td><%= tamu.tanggal %></td>
        <td><%= tamu.hotel %></td>
        <td><%= tamu.negara %></td>
        <td><%= tamu.telp %></td>
        <td><%= tamu.email %></td>
        <td><%= tamu.note %></td>
        <td><%= tamu.jenispaket %></td>
        <td><%= tamu.namaaktifitas %></td>
        <td><%= tamu.supplier %></td>
        <td><%= link_to 'Show', tamu %></td>
        <td><%= link_to 'Edit', edit_tamu_path(tamu) %></td>
        <td><%= link_to 'Destroy', tamu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tamu', new_tamu_path %>
