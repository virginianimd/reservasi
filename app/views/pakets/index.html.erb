<%= render partial: 'structures/navigation' %>  

<p id="notice"><%= notice %></p>

<h1>Pakets</h1>

<table class="table">
  <thead class="thead-dark">
    <tr align="center">
      <th>Jenispaket</th>
      <th>Desk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pakets.each do |paket| %>
      <tr>
        <td><%= paket.jenispaket %></td>
        <td><%= paket.desk %></td>
        <td><%= link_to 'Show', paket, class: 'btn btn-mini' %></td>
        <td><%= link_to 'Edit', edit_paket_path(paket), class: 'btn btn-mini btn-success' %></td>
        <td><%= link_to 'Destroy', paket, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-mini btn-danger'  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paket', new_paket_path %>
