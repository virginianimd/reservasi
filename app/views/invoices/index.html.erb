<%= render partial: 'structures/navigation' %>  

<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Noinvoice</th>
      <th>Tanggal</th>
      <th>Supplier</th>
      <th>Telp</th>
      <th>Paymentdue</th>
      <th>Desk</th>
      <th>Namatamu</th>
      <th>Namaaktifitas</th>
      <th>Tglaktifitas</th>
      <th>Qty</th>
      <th>Totalamount</th>
      <th>Namaakun</th>
      <th>Nomorakun</th>
      <th>Swiftcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice.noinvoice %></td>
        <td><%= invoice.tanggal %></td>
        <td><%= invoice.supplier %></td>
        <td><%= invoice.telp %></td>
        <td><%= invoice.paymentdue %></td>
        <td><%= invoice.desk %></td>
        <td><%= invoice.namatamu %></td>
        <td><%= invoice.namaaktifitas %></td>
        <td><%= invoice.tglaktifitas %></td>
        <td><%= invoice.qty %></td>
        <td><%= invoice.totalamount %></td>
        <td><%= invoice.namaakun %></td>
        <td><%= invoice.nomorakun %></td>
        <td><%= invoice.swiftcode %></td>
        <td><%= link_to 'Show', invoice %></td>
        <td><%= link_to 'Edit', edit_invoice_path(invoice) %></td>
        <td><%= link_to 'Destroy', invoice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice', new_invoice_path %>
